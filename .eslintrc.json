{
  "extends": ["conventions", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "unicorn"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "unicorn/prefer-node-protocol": "off"
  }
}
