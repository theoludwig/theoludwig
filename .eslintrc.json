{
  "extends": ["conventions", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "unicorn"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parser": "@typescript-eslint/parser"
    }
  ]
}
