{
  "name": "theoludwig",
  "version": "2.12.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/theoludwig/theoludwig"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "npm run resume:build && next build",
    "lint:commit": "commitlint",
    "lint:editorconfig": "editorconfig-checker",
    "lint:markdown": "markdownlint-cli2",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:prettier": "prettier . --check",
    "lint:staged": "lint-staged",
    "test:unit": "cypress run --component",
    "test:html-w3c-validator": "start-server-and-test \"start\" \"http://127.0.0.1:3000\" \"html-w3c-validator\"",
    "test:e2e": "start-server-and-test \"start\" http://127.0.0.1:3000 \"cypress run\"",
    "test:dev": "start-server-and-test \"dev\" \"http://127.0.0.1:3000\" \"cypress open\"",
    "resume:build": "node ./jsonresume-theme-custom/build.js",
    "release": "semantic-release",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@fontsource/montserrat": "5.0.5",
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@giscus/react": "2.3.0",
    "clsx": "1.2.1",
    "date-and-time": "3.0.2",
    "gray-matter": "4.0.3",
    "html-react-parser": "4.0.0",
    "katex": "0.16.8",
    "next": "13.4.10",
    "next-mdx-remote": "4.4.1",
    "next-themes": "0.2.1",
    "next-translate": "2.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "read-pkg": "8.0.0",
    "rehype-katex": "6.0.3",
    "rehype-raw": "6.1.1",
    "rehype-slug": "5.1.0",
    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "sharp": "0.32.3",
    "shiki": "0.14.3",
    "unified": "10.1.2",
    "unist-util-visit": "5.0.0",
    "universal-cookie": "4.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@saithodev/semantic-release-backmerge": "3.2.0",
    "@semantic-release/git": "10.0.1",
    "@tailwindcss/typography": "0.5.9",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/unist": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "10.4.14",
    "cypress": "12.17.1",
    "editorconfig-checker": "5.1.1",
    "eslint": "8.44.0",
    "eslint-config-conventions": "10.0.0",
    "eslint-config-next": "13.4.10",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "47.0.0",
    "html-w3c-validator": "1.4.0",
    "husky": "8.0.3",
    "jsonresume-theme-custom": "file:./jsonresume-theme-custom",
    "lint-staged": "13.2.3",
    "markdownlint-cli2": "0.8.1",
